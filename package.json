{
  "private": true,
  "author": "poyoho",
  "description": "A Library for shared",
  "scripts": {
    "serve": "cd playground/serve && yarn && yarn dev",
    "vue": "cd playground/vue && yarn && yarn dev",
    "react": "cd playground/react && yarn && yarn dev",
    "bootstrap": "lerna bootstrap",
    "lint": "eslint ./packages --ext .vue,.js,.ts",
    "lint-fix": "eslint --fix ./packages --ext .vue,.js,.ts,.tsx",
    "build": "node script/build.esm.js",
    "release": "node script/release.js",
    "build:wasm": "node script/build.wasm.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{ts,vue,tsx,jsx}": "eslint --fix"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "ISC",
  "devDependencies": {
    "@commitlint/config-conventional": "^11.0.0",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "@web/rollup-plugin-import-meta-assets": "^1.0.6",
    "chalk": "^4.1.0",
    "enquirer": "^2.3.6",
    "eslint": "^7.7.0",
    "eslint-plugin-vue": "^7.0.0-beta.0",
    "fs-extra": "^10.0.0",
    "lerna": "^3.22.1",
    "minimist": "^1.2.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.45.2",
    "rollup-plugin-dts": "^3.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "semver": "^7.3.2",
    "typescript": "^4.1.2"
  },
  "workspaces": [
    "packages/*"
  ],
  "files": [
    "dist",
    "packages"
  ],
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "version": "0.0.5"
}
